    "test:html": "vitest run --coverage",
    "prepublishOnly": "pnpm build"
  },
  "peerDependencies": {
    "vite": "^5.0.0"
  },
  "dependencies": {
    "json5": "^2.2.3",
    "yaml": "^2.5.0"
  },
  "devDependencies": {
    "@types/node": "^20.15.0",
    "@vitest/coverage-v8": "^3.0.9",
    "unbuild": "^3.5.0",
    "vite": "^5.4.1",
    "vitest": "^3.0.9"
  }
